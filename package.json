{
  "name": "@devskyui/react-video-thumbnail-extractor",
  "description": "Sky UI Components - React component for extracting thumbnails from video",
  "keywords": [
    "sky-ui",
    "react-components",
    "react-video-thumbnail",
    "react-video",
    "video-thumbnail-extractor"
  ],
  "license": "MIT",
  "type": "module",
  "types": "dist/index.d.ts",
  "main": "dist/react-video-thumbnail-extractor.umd.js",
  "module": "dist/react-video-thumbnail-extractor.es.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/react-video-thumbnail-extractor.umd.js",
      "import": "./dist/react-video-thumbnail-extractor.es.js",
      "style": "./dist/style.css"
    },
    "./style.css": "./dist/style.css"
  },
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "version": "0.1.1",
  "author": "Haneul Cho <stuykr@naver.com> (https://github.com/haneulcho)",
  "homepage": "https://haneulcho.github.io/react-video-thumbnail-extractor",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/haneulcho/react-video-thumbnail-extractor.git"
  },
  "bugs": {
    "url": "https://github.com/haneulcho/react-video-thumbnail-extractor/issues"
  },
  "scripts": {
    "dev": "vite --config vite.examples.config.ts",
    "build": "vite build",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "format": "yarn-or-npm prettier --check --debug-check \"{src,examples}/**/*.{ts,tsx}\"",
    "format:fix": "yarn-or-npm prettier --write \"{src,examples}/**/*.{ts,tsx}\"",
    "lint": "eslint \"{src,examples}/**/*.{ts,tsx}\" --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "yarn-or-npm lint --fix",
    "check-types": "tsc --noEmit --pretty",
    "docs": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs --port 8080",
    "postversion": "yarn-or-npm build"
  },
  "dependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-player": "^2.0.0",
    "react-hot-toast": "^2.4.1",
    "react-icons": "^5.3.0",
    "react-quill-new": "^3.3.3"
  },
  "devDependencies": {
    "@eslint/js": "9.14.0",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@testing-library/react-hooks": "8.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^22.8.1",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.1",
    "@types/react-transition-group": "^4.4.11",
    "@vitejs/plugin-react": "^4.3.3",
    "@vitejs/plugin-react-swc": "^3.7.1",
    "@vitest/ui": "^2.1.3",
    "autoprefixer": "^10.4.20",
    "clsx": "^2.1.1",
    "eslint": "9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-tailwindcss": "^3.17.5",
    "eslint-plugin-unused-imports": "^4.1.4",
    "jsdom": "^25.0.1",
    "postcss": "^8.4.47",
    "postcss-import": "^16.1.0",
    "prettier": "3.1.1",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-player": "^2.0.0",
    "react-transition-group": "^4.4.5",
    "tailwind-merge": "^2.5.4",
    "tailwindcss": "^3.4.14",
    "typescript": "5.4.2",
    "typescript-eslint": "^8.14.0",
    "vite": "^5.4.11",
    "vite-plugin-dts": "^4.3.0",
    "vite-plugin-mkcert": "^1.17.6",
    "vite-tsconfig-paths": "^5.1.2",
    "vitepress": "^1.5.0",
    "vitest": "^2.1.5",
    "yarn-or-npm": "^3.0.1"
  },
  "engines": {
    "node": ">=20.18.0"
  },
  "directories": {
    "doc": "docs",
    "example": "examples"
  }
}
